<!DOCTYPE html>
<html lang="en">
<!-- Basic -->

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Site Metas -->
    <title>Freshshop - Ecommerce Bootstrap 4 HTML Template</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">

    <!-- Site Icons -->
    <link rel="shortcut icon" href="~/freshshop/images/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="~/freshshop/images/apple-touch-icon.png">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/freshshop/css/bootstrap.min.css">
    <!-- Site CSS -->
    <link rel="stylesheet" href="~/freshshop/css/style.css">
    <!-- Responsive CSS -->
    <link rel="stylesheet" href="~/freshshop/css/responsive.css">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/freshshop/css/custom.css">

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>
    <!-- Start Main Top -->
    <div class="main-top">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                    <div class="right-phone-box">
                        <p> <a href="#"> </a></p>
                    </div>
                    <div class="our-link">
                        <ul>
                            <li><a href="#"><i class="fa fa-user s_color"></i> My Account</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                    <div class="login-box">
                        <select id="basic" class="selectpicker show-tick form-control" data-placeholder="Sign In">
                            <option>Register Here</option>
                            <option>Sign In</option>
                        </select>
                    </div>
                    <div class="text-slid-box">
                        <div id="offer-box" class="carouselTicker">
                            <ul class="offer-box">
                                <li>
                                    <i class="fab fa-opencart"></i> Organik Ürünler Pazarı
                                </li>
                                <li>
                                    <i class="fab fa-opencart"></i> Organik Meyvelerde 50% - 80% Varan İndirimler
                                </li>
                                <li>
                                    <i class="fab fa-opencart"></i> Zeytin Kategorisinde 10% İndirim!
                                </li>
                                <li>
                                    <i class="fab fa-opencart"></i> Organik beslen sağlığını koru.
                                </li>
                                <li>
                                    <i class="fab fa-opencart"></i> Sağlık için her şeye değer.
                                </li>
                                <li>
                                    <i class="fab fa-opencart"></i> Süt Ürünlerinde 40% - 60% Varan İndirimler
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    @await Html.PartialAsync("_HeaderPartial")

    <div class="products-box">
        @RenderBody()
    </div>
    @await Html.PartialAsync("_FooterPartial")

    <!-- Start copyright  -->
    <div class="footer-copyright">
        <p class="footer-company">
            GreenDelight & 2024 <a href="#"></a>- Yazılımcı :
            <a href="#">Rabia Nur Kavi</a>
        </p>
    </div>
    <!-- End copyright  -->

    <a href="#" id="back-to-top" title="Back to top" style="display: none;">&uarr;</a>

    <!-- ALL JS FILES -->
    <script src="~/freshshop/js/jquery-3.2.1.min.js"></script>
    <script src="~/freshshop/js/popper.min.js"></script>
    <script src="~/freshshop/js/bootstrap.min.js"></script>
    <!-- ALL PLUGINS -->
    <script src="~/freshshop/js/jquery.superslides.min.js"></script>
    <script src="~/freshshop/js/bootstrap-select.js"></script>
    <script src="~/freshshop/js/inewsticker.js"></script>
    <script src="~/freshshop/js/bootsnav.js."></script>
    <script src="~/freshshop/js/images-loded.min.js"></script>
    <script src="~/freshshop/js/isotope.min.js"></script>
    <script src="~/freshshop/js/owl.carousel.min.js"></script>
    <script src="~/freshshop/js/baguetteBox.min.js"></script>
    <script src="~/freshshop/js/form-validator.min.js"></script>
    <script src="~/freshshop/js/contact-form-script.js"></script>
    <script src="~/freshshop/js/custom.js"></script>
</body>

</html>